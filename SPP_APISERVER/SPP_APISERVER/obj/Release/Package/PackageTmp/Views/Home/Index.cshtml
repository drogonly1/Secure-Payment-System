<script language="javascript">
    var m = 0;
    var s = 10;
    var timeout = null; // Timeout
    window.onload = start;
    function start() {
        if (s === -1) {
            m -= 1;
            s = 59;
        }

        if (m === -1) {
            console.log("OUT");
            load_ajax();
            return;
        }

        var text = "0" + m.toString() + ":" + s.toString();
        $("#timeout").text(text);
        timeout = setTimeout(function () {
            s--;
            start();
        }, 1000);
    }
    function load_ajax() {
        
        $.ajax({
            type: "POST",
            url: "https://localhost:44327/api/Error?status=timeOut&transId=",
            success: function (msg) {
                console.log(msg)
            }
        });
    }
</script>
<br />
<p style="font-size:18px;" id="timeout"></p>
